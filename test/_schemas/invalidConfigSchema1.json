{
  "$schema": "../../schemas/open-microfrontends.json",
  "openMicrofrontends": "1.0.0",
  "microfrontends": [{
    "name": "My First Microfrontend",
    "resources": {
      "js": ["app.js"]
    },
    "paths": {
      "resourcesBase": "/"
    },
    "globalLaunchFunction": "startMyFirstMicrofrontend",
    "config": {
      "schema": {
        "type": "object",
        "properties": {
          "customerId": {
            "typex": "string"
          }
        },
        "required": [
          "customerId"
        ],
        "additionalProperties": false
      },
      "default": {
        "customerId": "1000"
      }
    }
  }]
}
